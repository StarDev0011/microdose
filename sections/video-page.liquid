{% schema %}
{
  "name": "Video Page Section",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "header",
      "content": "t:sections.all.padding.section_padding_heading"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 36
    },
    {
      "type": "text",
      "id": "video",
      "label": "video Url Mp4"
    },
    {
      "type": "image_picker",
      "id": "img",
      "label": "video Thumbnail"
    },
    {
      "type": "text",
      "id": "title",
      "label": "Title"
    },
    {
      "type": "textarea",
      "id": "content",
      "label": "Content"
    },
    {
      "type": "richtext",
      "id": "videos_title",
      "label": "Videos Box Title"
    },
    {
      "type": "textarea",
      "id": "videos_content",
      "label": "Videos Box Content"
    },
    {
      "type": "url",
      "id": "link",
      "label": "wholesale Button link"
      
    }
  ],
    "blocks": [
    {
      "type": "videos",
      "name": "Video Clip",
      "settings": [
      {
      "type": "text",
      "id": "videos_mp4",
      "label": "Video mp4 url"
       },
        {
      "type": "image_picker",
      "id": "videos_thumbnail",
      "label": "Video Thumbnail"
       },
       {
      "type": "text",
      "id": "title",
      "label": "Title"
       },
       {
      "type": "richtext",
      "id": "content",
      "default": "<p>Add Content here...</p>",
      "label": "Descriptions"
       },
       {
      "type": "text",
      "id": "embed",
      "label": "vimeo Embed code"
       },
       {
      "type": "text",
      "id": "thumb",
      "label": "vimeo thumbnail url"
       }
        
      ]
    }
    ]
  
}
{% endschema %}
